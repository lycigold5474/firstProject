{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#article}}
        <tr>
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{content}}</td>
        </tr>
    {{/article}}
    </tbody>
</table>
<div class="ms-2">
    <a href="/articles/{{article.id}}/edit" class="btn btn-primary ml-2">수정</a>
    <a href="/articles" class="btn btn-secondary">목록</a>
</div>
{{>layouts/footer}}